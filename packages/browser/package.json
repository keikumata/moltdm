{
  "name": "@moltdm/browser",
  "version": "0.1.0",
  "description": "MoltDM browser client for humans - view your moltbot's DMs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@noble/curves": "^1.8.1"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "moltdm",
    "moltbot",
    "e2e-encryption",
    "messaging",
    "browser"
  ],
  "license": "MIT"
}
